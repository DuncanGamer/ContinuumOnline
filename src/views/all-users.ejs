<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Document</title>
</head>
<body>
<nav>
    <div class="mobile">
      <div class="header">
        <div class="logo"><img class="logo" src="./images/logo.png"> </div>
        <div class="more"><button id="bMore"><span class="material-icons">menu</span></button></div>
      </div>
      <div id="links" class ="active">
        <a href="/">Home</a>
        <a href="#">Documentation</a>
        <a href="/users/all-users">All users</a>
        <a href="#">Create users</a>
        <a href="#">All concerts</a>
        <a href="/users/create-concert">Create concert</a>
        <a href="#">All tickets</a>
        <a href="#">Create ticket</a>
        <a href="#">All reservations</a>
        <a href="#">Create reservation</a>
      </div>





    </div>

    <div class="desktop">
      <div class="logo"><img class="logo" src="../images/logo.png"></div>
      <div class="primary">
        <a href="/">Home</a>
        <a href="#">Documentation</a>
        <a href="/users/all-users">All users</a>
      </div>

      <div class="secondary full">
        <a href="#">Serch</a>
        <a href="#">Sign up</a>
        <a href="#">Login</a>
      </div>

      <div class="secondary mini">
        <a href="#">More</a>
        <div class="submenu">
          <a href="#">Serch</a>
          <a href="#">Sign up</a>
          <a href="#">Login</a>
        </div>
      </div>
    </div>
  </nav>  
    

<table>
    <tr>
        <td>Id</td>
        <td>Name</td>
        <td>Email</td>
        <td>Pasword</td>
        <td>Modificar</td>
        <td>Eliminar</td>
    </tr>
   <%users.forEach((user) => {  %>
        <tr>
            <td><%=user.id%></td>
            <td><%=user.name%></td>
            <td><%=user.email%></td>
            <td><%=user.password%></td>
            <td><a id='btn-update' href="/users/update-users/<%=user.id%>">Update</a></td>
            <td><a id='btn-delete' href="/users/delete-users/<%=user.id%>">Delete</a></td>
        </tr>
             <% }) %>

</table> 

<script src="/js/script.js"></script>
</body>
</html>